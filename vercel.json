{
  "version": 2,
  "buildCommand": "cd frontend && npm install",
  "outputDirectory": "frontend",
  "framework": "static",
  "installCommand": "npm install",
  "public": true,
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }],
  "github": {
    "silent": true,
    "autoAlias": true
  }
}
